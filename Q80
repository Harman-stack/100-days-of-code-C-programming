Q80: Multiply two matrices.

/*
Sample Test Cases:
Input 1:
2 3
1 2 3
4 5 6
3 2
7 8
9 10
11 12
Output 1:
58 64
139 154

*/

#include <stdio.h>

int main()
{
    int R1, C1, R2, C2, i, j, k;
    scanf("%d %d", &R1, &C1);

    int A[R1][C1];
    for (i = 0; i < R1; i++)
        for (j = 0; j < C1; j++)
            scanf("%d", &A[i][j]);

    scanf("%d %d", &R2, &C2);

    int B[R2][C2];
    for (i = 0; i < R2; i++)
        for (j = 0; j < C2; j++)
            scanf("%d", &B[i][j]);

    if (C1 != R2) return 1;

    int C[R1][C2];

    for (i = 0; i < R1; i++)
    {
        for (j = 0; j < C2; j++)
        {
            C[i][j] = 0;
            for (k = 0; k < C1; k++)
                C[i][j] += A[i][k] * B[k][j];
        }
    }

    for (i = 0; i < R1; i++)
    {
        for (j = 0; j < C2; j++)
            printf("%d%c", C[i][j], (j == C2 - 1) ? '\n' : ' ');
    }

    return 0;
}
